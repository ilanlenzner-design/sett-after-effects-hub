<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="7.0" ExtensionBundleId="com.sett.bgremover" ExtensionBundleVersion="1.0.0"
                   ExtensionBundleName="BG Remover" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <Author>Sett.ai</Author>
  <ExtensionList>
    <Extension Id="com.sett.bgremover" Version="1.0.0"/>
  </ExtensionList>
  <ExecutionEnvironment>
    <HostList>
      <Host Name="AEFT" Version="[0.0,99.9]"/>
    </HostList>
    <LocaleList>
      <Locale Code="All"/>
    </LocaleList>
    <RequiredRuntimeList>
      <RequiredRuntime Name="CSXS" Version="9.0"/>
    </RequiredRuntimeList>
  </ExecutionEnvironment>
  <DispatchInfoList>
    <Extension Id="com.sett.bgremover">
      <DispatchInfo>
        <Resources>
          <MainPath>./index.html</MainPath>
          <ScriptPath>./hostscript.jsx</ScriptPath>
          <CEFCommandLine>
            <Parameter>--enable-nodejs</Parameter>
            <Parameter>--mixed-context</Parameter>
          </CEFCommandLine>
        </Resources>
        <Lifecycle>
          <AutoVisible>true</AutoVisible>
        </Lifecycle>
        <UI>
          <Type>Panel</Type>
          <Menu>BG Remover</Menu>
          <Geometry>
            <Size>
              <Height>600</Height>
              <Width>350</Width>
            </Size>
            <MinSize>
              <Height>400</Height>
              <Width>300</Width>
            </MinSize>
          </Geometry>
          <Icons>
          </Icons>
        </UI>
      </DispatchInfo>
    </Extension>
  </DispatchInfoList>
</ExtensionManifest>
